{
  "autoDocstring.docstringFormat": "google",
  "black-formatter.importStrategy": "fromEnvironment",
  "black-formatter.args": ["--line-length", "79"],
  "cSpell.diagnosticLevel": "Warning",
  "cSpell.userWords": [
    "dateutil",
    "docstrings",
    "getcontext",
    "gettz",
    "mypy",
    "paramiko",
    "planarize",
    "planarized",
    "prec",
    "pylance",
    "pyodide",
    "pypiwin",
    "pyscript",
    "pyspartaproj",
    "pytest",
    "pywin",
    "setcontext",
    "unmatch",
    "Targetpath",
    "isort",
    "pyenv",
    "venv",
    "levelname"
  ],
  "debug.console.fontFamily": "editor",
  "debug.console.fontSize": 13,
  "editor.codeActionsOnSave": {
    "source.organizeImports": true
  },
  "editor.fontFamily": "UDEV Gothic JPDOC",
  "editor.fontSize": 15,
  "editor.formatOnSave": true,
  "editor.minimap.enabled": false,
  "editor.rulers": [79],
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "string.quoted.docstring.multi.python",
        "settings": {
          "foreground": "#ffBB00"
        }
      }
    ]
  },
  "explorer.excludeGitIgnore": false,
  "flake8.importStrategy": "fromEnvironment",
  "flake8.severity": {
    "D": "Warning"
  },
  "files.exclude": {
    "**/__pycache__/": true,
    ".temp/.mypy_cache/": true,
    ".temp/.pytest_cache/": true,
    "poetry/linux/.venv/": true,
    "poetry/windows/.venv/": true
  },
  "git.autoRepositoryDetection": "openEditors",
  "isort.args": ["--settings=pyproject.toml"],
  "liveServer.settings.NoBrowser": true,
  "liveServer.settings.root": "/docs",
  "mypy-type-checker.args": ["--config-file=pyproject.toml"],
  "pythonTestExplorer.testFramework": "pytest",
  "python.analysis.diagnosticMode": "workspace",
  "python.analysis.diagnosticSeverityOverrides": {
    "reportShadowedImports": "information",
    "reportMissingTypeStubs": "information"
  },
  "python.analysis.extraPaths": ["docs", ".temp/typings"],
  "python.analysis.stubPath": ".temp/typings",
  "python.analysis.typeCheckingMode": "strict",
  "python.envFile": "",
  "python.languageServer": "Pylance",
  "python.testing.pytestEnabled": true,
  "terminal.integrated.fontSize": 14,
  "testing.gutterEnabled": false,
  "workbench.colorTheme": "Dracula Soft",
  "workbench.startupEditor": "none",
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  }
}
